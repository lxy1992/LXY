[alias]
    b = branch
    br = branch
    ci = commit
    cia = commit --all
    cim = commit --amend
    cima = commit --amend --all
    co = checkout
    crp = cherry-pick
    d = diff
    bd = branch -D
    dfl = diff --name-status
    dfs = diff --staged
    dl = diff HEAD^
    f = fetch --tags
    g = grep -in
    last = log -1 HEAD
    load = stash pop
    logg = log --graph --decorate --oneline --abbrev-commit
    logo = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    logs = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    mgf = merge --ff-only
    p = push
    pf = push -f
    puo = push -u origin
    rb = rebase
    rbc = rebase --continue
    regret = reset HEAD^
    rh = reset --hard
    rp = remote prune
    save = stash
    slog = log --graph --decorate --oneline --all --simplify-by-decoration
    squash = rebase HEAD~2 -i
    st = status
    yes = log --oneline --since yesterday --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[core]
    editor = vim
    pager = less -F -X
    autocrlf = false
    ignorecase = false
[color]
    ui = true
[merge]
    tool = vimdiff
[push]
    default = simple
[user]
    name = Xinyan Lv
    email = lvxinyan1992@gmail.com
